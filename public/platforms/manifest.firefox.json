{
  "manifest_version": 2,
  "name": "JSON Viewer",
  "content_scripts": [
    {
      "matches": [
        "http://*/*",
        "https://*/*",
        "ftp://*/*",
        "file:///*"
      ],
      "js": [
        "scripts/content.js"
      ],
      "run_at": "document_start",
      "all_frames": true
    }
  ],
  "icons": {
    "16": "icons/icon16.png",
    "64": "icons/icon64.png",
    "128": "icons/icon128.png"
  },
  "browser_action": {
    "default_icon": "icons/icon64.png"
  },
  "content_security_policy": "script-src 'self'; object-src 'self'",
  "web_accessible_resources": [
    "static/**/*",
    "index.html"
  ],
  "permissions": [
    "tabs"
  ],
  "browser_specific_settings": {
    "gecko": {
      "id": "json-viewer@misa198.com"
    }
  }
}